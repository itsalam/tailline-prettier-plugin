{
  "name": "prettier-plugin-tailline",
  "version": "0.0.3",
  "description": "A pretter plugin for TailwindCSS4.0 to sort classNames into groups and newlines",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "type": "module",
  "directories": {
    "dist": "./dist",
    "src": "./src",
    "test": "./src"
  },
  "scripts": {
    "example": "prettier --plugin . example.toml",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "watch": "parcel watch --no-content-hash",
    "build": "parcel build"
  },
  "keywords": [
    "prettier",
    "tailwindcss",
    "plugin"
  ],
  "author": {
    "name": "Vincent Lam",
    "email": "vincentthanhlam@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/parser": "^7.8.0",
    "escalade": "^3.1.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "3.2.5",
    "tailwindcss": "^4.0.0-alpha.14",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/types": "^7.24.5",
    "@eslint/js": "^9.1.1",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/types": "^7.8.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "parcel": "^2.12.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
